pico-8 cartridge // http://www.pico-8.com
version 36
__lua__
-- wanderkids
-- by anderkids

x=24 y=24 -- position (in tiles)
dx=0 dy=0 -- velocity
f=0       -- frame number
d=1       -- direction (-1, 1)
p=53       -- selected character
score={}
score.a=0
score.f=0

function _draw()
	cls(1)
	
	-- move camera to current room
	room_x = flr(x/16)
	room_y = flr(y/16)
	camera(room_x*128,room_y*128)
	
	-- draw the whole map (128⁙32)
	map()
	
	-- draw the player
	spr(p+f,      -- frame index
	 x*8-4,y*8-4, -- x,y (pixels)
	 1,1,d==-1    -- w,h, flip
	)
	-- add the hud
	print(score.a,room_x*128+10,room_y*128+1,7)
	print(score.f,room_x*128+30,room_y*128+1,7)
	spr(25,room_x*128+20,room_y*128+1)
	spr(9,room_x*128+1,room_y*128+1,1,1)
	
end

function _update()
	
	ac=0.1 -- acceleration
	
	if (btn(⬅️)) dx-= ac d=-1
	if (btn(➡️)) dx+= ac d= 1
	if (btn(⬆️)) dy-= ac
	if (btn(⬇️)) dy+= ac
	
	-- move (add velocity)
	x+=dx y+=dy
	
	-- friction (lower for more)
	dx *=.7
	dy *=.7
	
	-- advance animation according
	-- to speed (or reset when
	-- standing almost still)
	spd=sqrt(dx*dx+dy*dy)
	f= (f+spd*2) % 4 -- 4 frames
	if (spd < 0.05) f=0
	
	-- collect apple
	if (mget(x,y)==10) then
		mset(x,y,58)
		sfx(0)
		score.a+=1
	end
-- push tree
	if (mget(x,y)==31) then
		mset(x,y,47)
		sfx(5)
	--score.a+=1
	end
-- step on pine cone
	if (mget(x,y)==30) then
		mset(x,y,46)
		sfx(6)
	--score.a+=1
	end


	
	-- squish flower
		if (mget(x,y)==13) then
		mset(x,y,29)
		sfx(3)
		score.f+=1
	end

	-- change character to fire
		if (mget(x,y)==26) then
		p=17
		sfx(4)
	end

	-- change character to kuva
		if (mget(x,y)==42) then
		p=49
		sfx(4)
	end

	-- change character to babas
		if (mget(x,y)==27) then
		p=5
		sfx(4)
	end
	
		-- change character to kiy
		if (mget(x,y)==43) then
		p=21
		sfx(4)
	end

		-- change character to smoke
		if (mget(x,y)==28) then
		p=37
		sfx(4)
	end	

		-- change character to herry
		if (mget(x,y)==44) then
		p=65
		sfx(4)
	end	
	
end

